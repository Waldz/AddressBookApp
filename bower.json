  "main": "public/js/main.js",
  "dependencies": {
    "underscore": "~1.8.3",
    "jquery": "~2.1.4",
    "backbone": "~1.2.3",
    "bootstrap": "~3.3.5",
    "requirejs": "~2.1.20"
  },
  "install": {
    "path": "public/js/vendor",
    "sources": {
